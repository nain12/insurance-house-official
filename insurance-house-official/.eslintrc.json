{
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "settings": {
        "react": {
          "version": "detect"
          },
          "import/resolver": {
            "node": {
              "extensions": [".js", ".jsx"]
            }
          }
        },
    "extends": [
        "eslint:recommended",
        "plugin:import/errors",
        "plugin:react/recommended"
       /*  "plugin:jsx-a11y/recommended" */
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true
        }
    },
    "plugins": [
        "react", "import", "jsx-a11y"
    ],
    "rules": {
        "semi": 0,
        "indent": 2,
        "quotes": ["error", "double"],
        "multiline-ternary": ["error", "never"],
        "comma-dangle": ["error", "never"],
        "react/prop-types": 0
    }
}
